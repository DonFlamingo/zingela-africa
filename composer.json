{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"laravel/framework": "5.1.*",
        "predis/predis": "~1.0",
		"maatwebsite/excel": "~2.0.0",
		"shuber/curl": "dev-master",
		"bugsnag/bugsnag-laravel": "1.*",
		"illuminate/html": "~5.0",
		"netshell/paypal": "dev-master",
		"phpmailer/phpmailer": "~5.2",
		"fideloper/proxy": "^3.1",
		"stanley/geocodio-php": "~1.0",
		"sendgrid/sendgrid": "~5.0",
		"wildbit/postmark-php" : "^2.2",
		"mailgun/mailgun-php": "~2.1.2",
		"php-http/guzzle6-adapter": "^1.1",
		"plivo/plivo-php": "~1.1.5",
		"h4cc/wkhtmltopdf-amd64": "0.12.*",
		"barryvdh/laravel-snappy": "0.3.*",
		"brozot/laravel-fcm": "^1.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/",
			"Tobuli\\": "Tobuli/",
			"ModalHelpers\\" : "ModalHelpers",
			"Facades\\" : "Facades"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize",
            "php artisan view:clear"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist",
		"allow-plugins": {
			"kylekatarnls/update-helper": true
		}
	}
}
