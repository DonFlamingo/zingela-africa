<?php $__env->startSection('content'); ?>
    <?php foreach($devices as $device): ?>
        <div class="panel panel-default">
            <div class="panel-heading">
                <?php echo e(rtl(trans('front.report_type'), $data)); ?>: <?php echo e(rtl($types[$data['type']], $data)); ?>

                <div class="pull-right">
                    <small><?php echo e(rtl(trans('front.overspeeds_count'), $data)); ?>: <?php echo e(isset($items[$device['id']]) ? $items[$device['id']]->overspeeds_count : 0); ?></small>
                </div>
            </div>
            <div class="panel-body">
                <table class="table" style="margin-bottom: 0px">
                    <tbody>
                    <tr>
                        <td><strong><?php echo e(rtl(trans('validation.attributes.device_id'), $data)); ?>:</strong></td>
                        <td><?php echo e(rtl($device['name'], $data)); ?></td>
                        <th>&nbsp;</th>
                        <td>&nbsp;</td>
                    </tr>
                    <tr>
                        <th><?php echo rtl(trans('front.time_period'), $data); ?>:</th>
                        <td><?php echo e($data['date_from']); ?> - <?php echo e($data['date_to']); ?></td>
                        <th>&nbsp;</th>
                        <td>&nbsp;</td>
                    </tr>
                    </tbody>
                </table>
            </div>
            <div class="panel-body no-padding">
                <table class="table table-striped table-speed" style="margin-bottom: 0px">
                    <thead>
                    <tr>
                        <th><?php echo e(rtl(trans('front.start'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.end'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.duration'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.top_speed'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.average_speed'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.position'), $data)); ?></th>
                    </tr>
                    </thead>
                    <tbody>
                    <?php if(!isset($items[$device['id']])): ?>
                        <tr>
                            <td colspan="20"><?php echo e(rtl(trans('front.nothing_found_request'), $data)); ?></td>
                        </tr>
                    <?php else: ?>
                        <?php
                        $device_items = $items[$device['id']]->getItems();
                        ?>
                        <?php if(empty($items[$device['id']]->overspeeds_count)): ?>
                            <tr>
                                <td colspan="20"><?php echo e(rtl(trans('front.nothing_found_request'), $data)); ?></td>
                            </tr>
                        <?php else: ?>
                            <?php foreach($device_items as $item): ?>
                                <?php if($item['status'] == 1 && count($item['overspeeds'])): ?>
                                    <?php foreach($item['overspeeds'] as $overspeed): ?>
                                        <tr class="text_center">
                                            <td><?php echo e($overspeed['start']); ?></td>
                                            <td><?php echo e(isset($overspeed['end']) ? $overspeed['end'] : '-'); ?></td>
                                            <td><?php echo e(secondsToTime($overspeed['time'])); ?></td>
                                            <td><?php echo e($overspeed['top_speed']); ?> <?php echo e(rtl(trans("front.dis_h_{$items[$device['id']]->unit_of_distance}"), $data)); ?></td>
                                            <td><?php echo e($overspeed['average_speed']); ?> <?php echo e(rtl(trans("front.dis_h_{$items[$device['id']]->unit_of_distance}"), $data)); ?></td>
                                            <td><a href="http://maps.google.com/maps?q=<?php echo e($overspeed['position']['lat']); ?>,<?php echo e($overspeed['position']['lng']); ?>&t=m" target="_blank"><?php echo e($overspeed['position']['lat']); ?> &deg;, <?php echo e($overspeed['position']['lng']); ?> &deg;</a>
                                                <?php if(!empty($overspeed['position']['address'])): ?>
                                                    - <?php echo e(rtl($overspeed['position']['address'], $data)); ?>

                                                <?php endif; ?>
                                            </td>
                                        </tr>
                                    <?php endforeach; ?>
                                <?php endif; ?>
                            <?php endforeach; ?>
                        <?php endif; ?>
                    <?php endif; ?>
                    </tbody>
                </table>
            </div>
        </div>
    <?php endforeach; ?>
<?php $__env->stopSection(); ?>
<?php echo $__env->make('Frontend.Reports.parse.layout', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>