<?php $__env->startSection('content'); ?>
    <?php foreach($devices as $device): ?>
        <div class="panel panel-default">
            <div class="panel-heading">
                <?php echo e(rtl(trans('front.report_type'), $data)); ?>: <?php echo e(rtl($types[$data['type']], $data)); ?>

            </div>
            <div class="panel-body">
                <table class="table" style="margin-bottom: 0px">
                    <tbody>
                    <tr>
                        <td><strong><?php echo e(rtl(trans('validation.attributes.device_id'), $data)); ?>:</strong></td>
                        <td><?php echo e(rtl($device['name'], $data)); ?></td>
                        <th>&nbsp;</th>
                        <td>&nbsp;</td>
                    </tr>
                    <tr>
                        <th><?php echo rtl(trans('front.time_period'), $data); ?>:</th>
                        <td><?php echo e($data['date_from']); ?> - <?php echo e($data['date_to']); ?></td>
                        <th>&nbsp;</th>
                        <td>&nbsp;</td>
                    </tr>
                    </tbody>
                </table>
            </div>
            <div class="panel-body no-padding">
                <table class="table table-striped table-speed" style="margin-bottom: 0px">
                    <thead>
                    <tr>
                        <th><?php echo e(rtl(trans('validation.attributes.date'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.duration'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.position_a'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.position_b'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.route_length'), $data)); ?></th>
                        <th><?php echo e(rtl(trans('front.driver'), $data)); ?></th>
                        <?php if($device['fuel_per_km'] > 0): ?>
                            <th><?php echo e(rtl(trans('front.fuel_consumption'), $data)); ?> (<?php echo e(rtl(trans('front.gps'), $data)); ?>)</th>
                        <?php endif; ?>
                        <?php if(isset($items[$device['id']])): ?>
                            <?php foreach($items[$device['id']]->fuel_consumption as $id => $value): ?>
                                <th><?php echo e(rtl(trans('front.fuel_consumption'), $data)); ?> (<?php echo e(rtl($items[$device['id']]->sensors_arr[$id]['name'], $data)); ?>)</th>
                            <?php endforeach; ?>
                        <?php endif; ?>
                        <?php if($device['fuel_price'] > 0): ?>
                            <?php if($device['fuel_per_km'] > 0): ?>
                                <th><?php echo e(rtl(trans('front.fuel_cost'), $data)); ?> (<?php echo e(rtl(trans('front.gps'), $data)); ?>):</th>
                            <?php endif; ?>
                            <?php if(isset($items[$device['id']])): ?>
                                <?php foreach($items[$device['id']]->fuel_consumption as $id => $value): ?>
                                    <th><?php echo e(rtl(trans('front.fuel_cost'), $data)); ?> (<?php echo e($items[$device['id']]->sensors_arr[$id]['name']); ?>):</th>
                                <?php endforeach; ?>
                            <?php endif; ?>
                        <?php endif; ?>
                    </tr>
                    </thead>
                    <tbody>
                    <?php if(!isset($items[$device['id']])): ?>
                        <tr>
                            <td colspan="20"><?php echo e(rtl(trans('front.nothing_found_request'), $data)); ?></td>
                        </tr>
                    <?php else: ?>
                        <?php
                        $device_items = $items[$device['id']]->getItems();
                        //array_shift($device_items);
                        //array_pop($device_items);
                        $is = 0;
                        ?>
                        <?php if(empty($device_items)): ?>
                            <tr>
                                <td colspan="20"><?php echo e(rtl(trans('front.nothing_found_request'), $data)); ?></td>
                            </tr>
                        <?php else: ?>
                            <?php foreach($device_items as $item): ?>
                                <?php if($item['status'] == 1): ?>
                                    <tr>
                                        <td><?php echo e(datetime(date('Y-m-d H:i:s', strtotime($item['raw_time'])), FALSE)); ?></td>
                                        <td><?php echo e($item['time']); ?></td>
                                        <td>
                                        <a href="http://maps.google.com/maps?q=<?php echo e($item['start_position']['lat']); ?>,<?php echo e($item['start_position']['lng']); ?>&t=m" target="_blank">
                                            <?php if(!empty($item['start_position']['address'])): ?>
                                                <?php echo e(rtl($item['start_position']['address'], $data)); ?>

                                            <?php else: ?>
                                                <?php echo e($item['start_position']['lat']); ?> &deg;, <?php echo e($item['start_position']['lng']); ?> &deg;
                                            <?php endif; ?>
                                        </a>
                                        </td>
                                        <td>
                                        <a href="http://maps.google.com/maps?q=<?php echo e($item['stop_position']['lat']); ?>,<?php echo e($item['stop_position']['lng']); ?>&t=m" target="_blank">
                                            <?php if(!empty($item['stop_position']['address'])): ?>
                                                <?php echo e(rtl($item['stop_position']['address'], $data)); ?>

                                            <?php else: ?>
                                                <?php echo e($item['stop_position']['lat']); ?> &deg;, <?php echo e($item['stop_position']['lng']); ?> &deg;
                                            <?php endif; ?>
                                        </a>
                                        </td>
                                        <td><?php echo e($items[$device['id']]->unit_of_distance == 'mi' ? kilometersToMiles($item['distance']) : $item['distance']); ?> <?php echo e(rtl(trans('front.'.$items[$device['id']]->unit_of_distance), $data)); ?></td>
                                        <td><?php echo e(rtl($item['driver'], $data)); ?></td>
                                        <?php if($device['fuel_per_km'] > 0): ?>
                                            <td><?php echo e($device['fuel_measurement_id'] == 1 ? float($item['distance'] * $device['fuel_per_km']).' '.rtl(trans('front.liters'), $data) : number_format(litersToGallons(($item['distance'] * $device['fuel_per_km'])), 2, '.', '').' '.rtl(trans('front.gallons'), $data)); ?></td>
                                        <?php endif; ?>
                                        <?php foreach($items[$device['id']]->fuel_consumption as $id => $value): ?>
                                            <td><?php echo e(($value > 0 ? float((float($value)/$items[$device['id']]->distance_sum) * $item['distance']) : float($value)).' '.rtl($items[$device['id']]->sensors_arr[$id]['sufix'], $data)); ?></td>
                                        <?php endforeach; ?>

                                        <?php if($device['fuel_price'] > 0): ?>
                                            <?php if($device['fuel_per_km'] > 0): ?>
                                                <?php $fuel_consumption = $device['fuel_measurement_id'] == 1 ? $item['distance'] * $device['fuel_per_km'] : litersToGallons($item['distance'] * $device['fuel_per_km']); ?>
                                                <td><?php echo e(float($fuel_consumption * $device['fuel_price'])); ?></td>
                                            <?php endif; ?>
                                            <?php foreach($items[$device['id']]->fuel_consumption as $id => $value): ?>
                                                <?php $value = $value > 0 ? float((float($value)/$items[$device['id']]->distance_sum) * $item['distance']) : float($value); ?>
                                                <td><?php echo e(float($value * $device['fuel_price'])); ?></td>
                                            <?php endforeach; ?>
                                        <?php endif; ?>
                                    </tr>
                                <?php endif; ?>
                            <?php endforeach; ?>
                        <?php endif; ?>
                    <?php endif; ?>
                    </tbody>
                </table>
            </div>
            <?php if(isset($items[$device['id']])): ?>
            <div class="panel-body">
                <table style="margin-bottom: 0;" class="table">
                    <tr>
                        <th style="width: 260px;"><?php echo e(rtl(trans('front.route_length'), $data)); ?>:</th>
                        <td><?php echo e($items[$device['id']]->unit_of_distance == 'mi' ? kilometersToMiles($items[$device['id']]->distance_sum) : $items[$device['id']]->distance_sum); ?> <?php echo e(rtl(trans('front.'.$items[$device['id']]->unit_of_distance), $data)); ?></td>
                    </tr>
                    <tr>
                        <th style="width: 260px;"><?php echo e(rtl(trans('front.move_duration'), $data)); ?>:</th>
                        <td><?php echo e($items[$device['id']]->move_duration); ?></td>
                    </tr>
                </table>
            </div>
            <?php endif; ?>
        </div>
    <?php endforeach; ?>
<?php $__env->stopSection(); ?>
<?php echo $__env->make('Frontend.Reports.parse.layout', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>